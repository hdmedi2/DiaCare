{
  "name": "iyac_diabetes",
  "productName": "iyac_diabetes",
  "version": "1.0.17",
  "description": "아이약 당뇨케어",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development electron .",
    "start:prod": "cross-env NODE_ENV=prduction electorn .",
    "remove-dist": "node scripts/delete-dist-folder.js",
    "build": "npm run remove-dist && electron-builder",
    "clean-dist": "node scripts/delete-dist-folder.js",
    "clean-output": "node scripts/delete-release-folder.js",
    "obfuscate": "javascript-obfuscator src --output dist --compact true --control-flow-flattening true",
    "publish": "npm run clean-dist && npm run obfuscate && npm run clean-output && electron-builder --publish always",
    "version-up": "node version-bump.js",
    "make": "cross-env NODE_ENV=development npm run clean-dist && electron-builder"
  },
  "build": {
    "asar": true,
    "appId": "com.hdmedi.iyacDiaCare",
    "files": [
      "main.js",
      "config/**/*",
      "!scripts/**/*",
      "dist/**/*",
      "!src/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "hdmedi2",
        "repo": "DiaCareDist",
        "releaseType": "release"
      }
    ],
    "extraMetadata": {
      "main": "dist/index.js"
    },
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "./assets/iyac_app_logo.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "./assets/iyac_app_logo.icns"
    }
  },
  "repository": "github:hdmedi2/DiaCareDist",
  "keywords": [
    "당뇨케어"
  ],
  "author": "hdmedi, westwiz",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "javascript-obfuscator": "^4.1.1"
  },
  "dependencies": {
    "auto-update-test": "file:",
    "axios": "^1.7.7",
    "cross-env": "^7.0.3",
    "electron-log": "^5.2.2",
    "electron-updater": "^6.3.9",
    "iyac_diabetes": "file:",
    "json2csv": "^6.0.0-alpha.2",
    "playwright": "^1.49.0",
    "xmlhttprequest": "^1.8.0"
  }
}
